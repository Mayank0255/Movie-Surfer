<div style='margin-top: 25px;' class='col-md-4 col-sm-5 col-6 sectionls'>
    <%if (data.budget !== 0) {%>
        <h5>Budget </h5>
        <br>
    <%}%>
    <%if (data.budget !== 0) {%>
        <h5 style='margin-top: 5px;'>Revenue </h5>
        <br>
    <%}%>
    <h5>Languages</h5>
    <br><br><br>
    <h5>Production Companies</h5>
</div>
<div style='margin-top: 25px;' class='col-md-4 col-sm-5 col-6 sectionrs'>
    <%if (data.budget !== 0) {%>
    <h5 style='color: #ffffff;'>
        $<%=data.budget.toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,');%>
    </h5>
    <br>
    <%}%>
    <%if (data.budget !== 0) {%>
    <h5 style='color: #ffffff;'>
        $<%=data.revenue.toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,');%>
    </h5>
    <br>
    <%}%>
    <%data.spoken_languages.forEach(lang => {%>
    <h6 style='color: #ffffff; font-size: 18px;'>
        <%=lang.name%>
    </h6>
    <%});%>
    <br><br>
    <%data.production_companies.forEach(comp => {%>
    <h6 style='color: #ffffff; font-size: 18px;'>
        <%=comp.name%>
    </h6>
    <%});%>
</div>